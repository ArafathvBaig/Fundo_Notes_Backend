        "/api/forgotPassword": {
            "post": {
                "summary": "Forgot Password",
                "description": "Forgot Password for an user",
                "operationId": "f04608ed3c65a150ce67d9e12421b68c",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "email"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "email"
                                    }
                                },
                                "type": "object"
                            }
                        },
                        "application/json": {
                            "schema": {}
                        }
                    }
                },
                "responses": {
                    "404": {
                        "description": "Not a Registered Email"
                    },
                    "424": {
                        "description": "Email Not Sent"
                    },
                    "200": {
                        "description": "Reset Password Token Sent to your Email"
                    }
                }
            }
        },
        "/api/resetPassword": {
            "post": {
                "summary": "Reset User Password",
                "description": "Reset User Password using the token sent to the mail",
                "operationId": "bd437661db6f11362ce363815da51517",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
